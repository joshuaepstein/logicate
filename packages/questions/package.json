{
    "name": "@logicate/questions",
    "version": "0.0.1",
    "description": "Logicate question generator",
    "main": "./src/index.ts",
    "exports": {
        ".": "./src/index.ts",
        "./types/*": "./src/types/**/*.ts"
    },
    "dependencies": {
        "@ai-sdk/openai": "^0.0.54",
        "@ai-sdk/anthropic": "^0.0.48",
        "@logicate/utils": "workspace:*",
        "@logicate/database": "workspace:*",
        "ai": "^3.3.20",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@logicate/tsconfig": "workspace:*",
        "ts-node": "^10.9.2"
    },
    "scripts": {
        "generate:boolean-expression": "ts-node ./src/boolean-expression/generator.ts",
        "mark:boolean-expression": "dotenv -e ./.env -- ts-node ./src/boolean-expression/marker.ts",
        "generate:table": "ts-node ./src/boolean-expression/table-generator.ts"
    }
}